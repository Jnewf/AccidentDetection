<configuration>
    <input>
        <net-file value="accident.net.xml"/>
        <route-files value="accident.rou.xml"/>
	<gui-settings-file value="accident.settings.xml"/>
    </input>
    <time>
        <begin value="0"/>
        <end value="200"/>
        <step-length value="0.4"/>
    </time>
    <report>
        <verbose value="true"/>
        <no-step-log value="true"/> <!-- Optional: Reduces step logs to focus on collisions -->
    </report>
</configuration>
